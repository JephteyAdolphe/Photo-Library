<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="230.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jFiles.Controllers.userDeletePromptController">
   <children>
      <Label layoutX="40.0" layoutY="58.0" text="User To Be Deleted:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="userToDelete" layoutX="186.0" layoutY="58.0" />
      <Button layoutX="56.0" layoutY="165.0" mnemonicParsing="false" onMouseClicked="#confirmDeletion" text="Confirm Deletion" />
      <Button layoutX="224.0" layoutY="165.0" mnemonicParsing="false" onMouseClicked="#cancelPrompt" text="Cancel" />
      <TextField fx:id="typedUserToDelete" layoutX="101.0" layoutY="102.0" promptText="Retype UserName" />
   </children>
</AnchorPane>
